<div class="container mt-4 col-lg-4">
    <div class="card col-sm-8">
        <div class="card-body">
            <form class="form-sign" action="/pescaaki/pesqueiro/cadastro" method="POST">
                <div class="form-group text-center">
                    <h3>Cadastro</h3>
                    <label>Bem vindos ao Sistema</label>
                </div>
                <div class="form-group">
                    <label>Nome:</label>
                    <input type="text" name="nome" id="nome" value="" class="form-control">
                </div>
                <div class="form-group">
                    <label>Telefone:</label>
                    <input type="text" name="telefone" id="telefone" value="" class="form-control">
                </div>
                <div class="form-group">
                    <label>E-Mail:</label>
                    <input type="text" name="email" id="email" value="" class="form-control">
                </div>
                <div class="form-group">
                    <label>Descrição:</label>
                    <input type="text" name="descricao" id="descricao" value="" class="form-control">
                </div>
                <div class="form-group">
                    <label>Imagem:</label>
                    <input type="text" name="imagem" id="imagem" value="" class="form-control">
                </div>
                <input type="submit" name="btnLog" value="Cadastrar" id="btnLog" class="btn btn-primary btn-block">
            </form>
        </div>
    </div>
</div>